{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "bikeparking-twe",
        "runtime": "python3.8",
        "s3_bucket": "<ZAPPA_BUCKET>",
        "slim_handler": true,
        "include": [
            "secrets.py", "boundary.geojson"
        ],
        "keep_warm": false,
        "timeout_seconds": 600,
        "events": [{
          "function": "app.checkBikeParking",
          "expression": "cron(0 14-23 * * ? *)"
        }]
    }
}
